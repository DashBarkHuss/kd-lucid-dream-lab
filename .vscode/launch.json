{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Debug Generate Model Scores",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/sleep_scoring_toolkit/generate_model_scores.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "args": [
        "--input", "data/realtime_inference_test/8-16-25/BrainFlow-RAW_2025-08-16_04-33-14_0.csv",
        "--output", "sleep_stages_8-16-25_model_predictions.csv",
        "--board-id", "CYTON_DAISY_BOARD",
        "--montage", "minimal_sleep_montage",
        "--eeg-channels-for-scoring", "C4,C3,F3,F4",
        "--eog-channels-for-scoring", "L-HEOG,R-HEOG"
      ]
    },
    {
      "name": "(forced base env) Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "python": "/Users/dashiellbarkhuss/anaconda3/bin/python"
    },
    {
      "name": "Python: Debug Tests",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "purpose": ["debug-test"],
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Debug Main Speed Controlled Stream",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/sleep_scoring_toolkit/realtime_with_restart/main_speed_controlled_stream.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "justMyCode": false,
      "stopOnEntry": false
    },
    {
      "name": "Debug Main Realtime Stream",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/sleep_scoring_toolkit/realtime_with_restart/main_realtime_stream.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "justMyCode": false,
      "stopOnEntry": false
    }
  ]
}
